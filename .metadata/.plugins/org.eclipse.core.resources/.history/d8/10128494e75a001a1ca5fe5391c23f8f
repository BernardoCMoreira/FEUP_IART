package Levels;
import java.awt.Color;
import java.awt.Graphics;

import javax.swing.JPanel;

public class Level1 extends JPanel {
	
	public Level1(Graphics g) {
		
		g.setColor(Color.white);
	
		g.fillRect(200, 100, 45, 45);
		g.fillRect(250, 100, 45, 45);
		g.fillRect(300, 100, 45, 45);
		g.fillRect(350, 100, 45, 45);
		
		g.fillRect(200, 150, 45, 45);
		g.fillRect(250, 150, 45, 45);
		g.fillRect(300, 150, 45, 45);
		g.fillRect(350, 150, 45, 45);
		
		g.fillRect(150, 200, 45, 45);
		g.fillRect(200, 200, 45, 45);
		g.fillRect(250, 200, 45, 45);
		g.fillRect(300, 200, 45, 45);
		
		g.fillRect(150, 250, 45, 45);
		g.fillRect(200, 250, 45, 45);
		g.fillRect(250, 250, 45, 45);
		g.fillRect(300, 250, 45, 45);
		
	
	}
}
