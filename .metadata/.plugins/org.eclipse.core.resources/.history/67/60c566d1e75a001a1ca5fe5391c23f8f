package Levels;
import java.awt.Color;
import java.awt.Graphics;

import javax.swing.JPanel;

public class Level1 extends JPanel {
	
	public Level1(Graphics g) {
		
		g.setColor(Color.white);
	
		g.fillRect(200, 100, 95, 95);
		g.fillRect(300, 100, 95, 95);
		g.fillRect(400, 100, 95, 95);
		g.fillRect(500, 100, 95, 95);
		
		g.fillRect(200, 150, 95, 95);
		g.fillRect(250, 150, 95, 95);
		g.fillRect(300, 150, 95, 95);
		g.fillRect(350, 150, 95, 95);
				
		g.fillRect(200, 200, 95, 95);
		g.fillRect(250, 200, 95, 95);
		g.fillRect(300, 200, 95, 95);
		g.fillRect(350, 200, 95, 95);
		
		g.fillRect(200, 250, 95, 95);
		g.fillRect(250, 250, 95, 95);
		g.fillRect(300, 250, 95, 95);
		g.fillRect(350, 250, 95, 95);
		
	
	}
}
